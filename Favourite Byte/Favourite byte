from pwn import xor

given = '73626960647f6b206821204f21254f7d694f7624662065622127234f726927756d'

# Checks every XOR key
for key in range(256):
    # XOR the given
    plaintext = xor(bytes.fromhex(given), bytes([key]))
    # see if result is printable 
    # check if it starts 'crypto{'
    if all(32 <= b < 127 for b in plaintext) and plaintext.startswith(b'crypto{'):
        print(f'key: {key}, plaintext: {plaintext.decode()}')