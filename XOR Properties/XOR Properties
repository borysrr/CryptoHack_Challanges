from pwn import xor 

hex_Key_1 = 'a6c8b6733c9b22de7bc0253266a3867df55acde8635e19c73313'
hex_Key_2_1 = '37dcb292030faa90d07eec17e3b1c6d8daf94c35d4c9191a5e1e'
hex_Key_2_3 = 'c1545756687e7573db23aa1c3452a098b71a7fbf0fddddde5fc1'
hex_flag_1_3_2 = '04ee9855208a2cd59091d04767ae47963170d1660df7f56f5faf'

def hex_to_bytes(hex_str):
	byte_str = bytes.fromhex(hex_str)
	return byte_str

b_1 = hex_to_bytes(hex_Key_1)
b_2_1 = hex_to_bytes(hex_Key_2_1)
b_2_3 = hex_to_bytes(hex_Key_2_3)
b_Flag_1_3_2 = hex_to_bytes(hex_flag_1_3_2)

def xOr(arg1, arg2):
	res = xor(arg1, arg2)
	return res

flag_1 = xOr(b_Flag_1_3_2, b_2_3)
flag = xOr(flag_1, b_1).decode()
print('flag: ' f"{flag}")